<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph">

    <fragment
        android:id="@+id/HomeFragment"
        android:name="com.miyuan.smarthome.temp.HomeFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_HomeFragment_to_ScanFailFragment"
            app:destination="@id/ScanFailFragment"
            app:enterAnim="@anim/anim_right_in"
            app:exitAnim="@anim/anim_left_out"
            app:popEnterAnim="@anim/anim_left_in"
            app:popExitAnim="@anim/anim_right_out" />

        <action
            android:id="@+id/action_HomeFragment_to_NurseFragment"
            app:destination="@id/NurseFragment"
            app:enterAnim="@anim/anim_right_in"
            app:exitAnim="@anim/anim_left_out"
            app:popEnterAnim="@anim/anim_left_in"
            app:popExitAnim="@anim/anim_right_out" />

        <action
            android:id="@+id/action_HomeFragment_to_CreateFamilyMemberFragment"
            app:destination="@id/CreateFamilyMemberFragment"
            app:enterAnim="@anim/anim_right_in"
            app:exitAnim="@anim/anim_left_out"
            app:popEnterAnim="@anim/anim_left_in"
            app:popExitAnim="@anim/anim_right_out" />

        <action
            android:id="@+id/action_HomeFragment_to_RemindFragment"
            app:destination="@id/TempRemindAddFragment"
            app:enterAnim="@anim/anim_right_in"
            app:exitAnim="@anim/anim_left_out"
            app:popEnterAnim="@anim/anim_left_in"
            app:popExitAnim="@anim/anim_right_out" />

        <action
            android:id="@+id/action_HomeFragment_to_DisclaimerFragment"
            app:destination="@id/DisclaimerFragment"
            app:enterAnim="@anim/anim_right_in"
            app:exitAnim="@anim/anim_left_out"
            app:popEnterAnim="@anim/anim_left_in"
            app:popExitAnim="@anim/anim_right_out" />
    </fragment>

    <fragment
        android:id="@+id/ScanFailFragment"
        android:name="com.miyuan.smarthome.temp.ScanFailFragment"
        android:label="@string/scan_fail_fragment_label"
        tools:layout="@layout/fragment_scan_fail">

        <action
            android:id="@+id/action_ScanFailFragment_to_HomeFragment"
            app:destination="@id/HomeFragment"
            app:enterAnim="@anim/anim_right_in"
            app:exitAnim="@anim/anim_left_out"
            app:popEnterAnim="@anim/anim_left_in"
            app:popExitAnim="@anim/anim_right_out" />
    </fragment>

    <fragment
        android:id="@+id/NurseFragment"
        android:name="com.miyuan.smarthome.temp.NurselFragment"
        android:label="nurse Fragment"
        tools:layout="@layout/fragment_nurse">

    </fragment>


    <fragment
        android:id="@+id/CreateFamilyMemberFragment"
        android:name="com.miyuan.smarthome.temp.CreateFamilyMemberFragment"
        android:label="create family member Fragment"
        tools:layout="@layout/fragment_create_family_member">

    </fragment>

    <fragment
        android:id="@+id/TempRemindAddFragment"
        android:name="com.miyuan.smarthome.temp.TempRemindAddFragment"
        android:label="temp remind Fragment"
        tools:layout="@layout/fragment_temp_remind_add">

    </fragment>

    <fragment
        android:id="@+id/DisclaimerFragment"
        android:name="com.miyuan.smarthome.temp.DisclaimerFragment"
        android:label="disclaimer Fragment"
        tools:layout="@layout/fragment_disclaimer">

        <action
            android:id="@+id/action_DisclaimerFragment_to_HomeFragment"
            app:destination="@id/HomeFragment"
            app:enterAnim="@anim/anim_right_in"
            app:exitAnim="@anim/anim_left_out"
            app:popEnterAnim="@anim/anim_left_in"
            app:popExitAnim="@anim/anim_right_out"
            app:popUpTo="@id/DisclaimerFragment"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/TempRemindListFragment"
        android:name="com.miyuan.smarthome.temp.TempRemindListFragment"
        android:label="temp remind list Fragment"
        tools:layout="@layout/fragment_temp_remind_list">


    </fragment>

</navigation>